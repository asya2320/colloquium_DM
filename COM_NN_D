int COM_NN_D(char a[45], char b[45])
{
	int m(0), n(0);
	for (int i = 0; a[i] != '\0'; ++i)
	{
		m =( (m + a[i])-48) * 10;
	}
	for (int i = 0; b[i] != '\0'; ++i)
	{
		n = ((n + b[i])-48) * 10;
	}
	m = m / 10;
	n = n / 10;
	if (m < n)
	{
		m = 1;
	}
	if (m > n)
	{
		m = 2;
	}
	if (m == n)
	{
		m = 0;
	}
	return m;
}
