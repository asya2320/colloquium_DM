void MUL_QQ_Q(bool sign_1, char *numerator_1, char *denominator_1, bool sign_2, char *numerator_2, char *denominator_2, bool &sign_a, char *numerator_a, char *denominator_a)
{
	bool sign_D1 = 0, sign_D2 = 0, sign_Da;
	MUL_ZZ_Z(sign_1, numerator_1 ,&sign_a , numerator_a,sign_2 , numerator_2);
	MUL_ZZ_Z(sign_D1,denominator_1,&sign_Da,denominator_a,sign_D2,denominator_2);
}
