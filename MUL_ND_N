void MUL_ND_N(char a[], char ans[], int *lena, int D)
{

	
	for (int i = 0; i < *lena; i++)
	 ans[i] = a[i];
	ans[*lena] = '\0';
	for (int i = 0; i < *lena; i++)
		ans[i] = ans[i] - 48;
	

	TRANS(ans, *lena-1);
	
	int j;
	for (int i = 0; i < *lena; i++) ans[i] *= D;

	for (j = 0; j < *lena; j++)
	{
		ans[j + 1] += ans[j] / 10;
		ans[j] %= 10;
	}
	if (ans[*lena] > 0) ++*lena;

	
	for (int i = 0; i < *lena; i++)
	ans[i] = ans[i] + 48;
	TRANS(ans, *lena - 1);
	ans[*lena] = '\0';
}
